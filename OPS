(switch:: <arg> 
    (case:: <val> <body>)
    (case:: <val> <body>)
    (default:: <val> <body>))

(switch:: <arg> . <cases>)

(evcase <arg> exp env)

(while:: <exp> . <body>)


(cond:: (<predicate> <consequent>)
        (<predicate> <consequent>)
        (<predicate> <consequent>)
        (<predicate> <consequent>)
        (else <body>))
